#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/bobringer/work/bob-obringer/ods/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/bobringer/work/bob-obringer/ods/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../default-browser-id@1.0.4/node_modules/default-browser-id/cli.js" "$@"
else
  exec node  "$basedir/../../../../../default-browser-id@1.0.4/node_modules/default-browser-id/cli.js" "$@"
fi
